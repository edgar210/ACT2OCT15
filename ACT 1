int y1 = 0;

void setup(){
size(500,500);
	}
void draw(){
	background(250);
line(250,y1,250,250);
line(0,500,250,250);
line(500,500,250,250);
	y1++;
	}

int y2 = 0;
void setup(){
size(500,500);
	}

void draw(){
	background(250);
line(250,0,250,250);
	
line(0,500,250,y2);
line(500,500,250,250);
	y2++;
	}
